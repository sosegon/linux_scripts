[alias]
	st = status
	ps = push origin
	cm = commit -m
	df = diff
	ch = checkout
	lgo = log --pretty=format:%Cred%an%n%Cgreen%h%n%s
	lgf = log --pretty=format:%Cred%h%n%Cgreen%an%n---%s%n%n%b
	ad = add
	br = branch
	sh = stash
	pl = pull
	rshs = reset --soft HEAD^
	rshs = reset --hard HEAD^
	chf = checkout -- file
